{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.(ts|tsx)?$": "ts-jest"
  },
  "transformIgnorePatterns": ["<rootDir>/node_modules/"],
  "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
  ],
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1",
    "\\.(css|less)$": "identity-obj-proxy"
  }
}

