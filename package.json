{
  "name": "digitalocean-foundry-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watch",
    "lint": "eslint src/**/*.ts src/**/*.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,json} --config .prettierrc.json",
    "preview": "vite preview",
    "prepare": "husky install",
    "deploy": "tsc && vite build && surge ./dist dnd.t2pellet.me"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.16",
    "@mui/styled-engine-sc": "^5.10.16",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/google.maps": "^3.50.1",
    "@types/jest": "^29.1.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-simple-maps": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-cli": "^6.26.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.1.2",
    "jira-prepare-commit-msg": "^1.6.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "react-router-dom": "^6.4.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.0"
  }
}
